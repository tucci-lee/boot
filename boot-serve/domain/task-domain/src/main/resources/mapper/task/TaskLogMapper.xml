<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tuccicode.boot.domain.task.mapper.TaskLogMapper">

    <sql id="ALL">
        id
        , task_id, status, message, start_time, run_time, create_time
    </sql>

    <select id="selectPage" resultType="com.tuccicode.boot.domain.task.dataobject.TaskLogDO">
        SELECT
            <include refid="ALL"/>
        FROM
            task_log
        WHERE
            task_id = #{q.taskId}
        ORDER BY id DESC
    </select>
</mapper>